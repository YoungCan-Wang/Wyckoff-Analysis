# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_anon_key
# Step4 脚本建议配置 service role（USER_LIVE 读写）
SUPABASE_SERVICE_ROLE_KEY=

# Feishu Webhook (Optional)
FEISHU_WEBHOOK_URL=

# Tushare Pro (Optional, for data source)
TUSHARE_TOKEN=

# LLM (for step3 / step4)
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash

# Step4 private portfolio + Telegram delivery
# Example:
# MY_PORTFOLIO_STATE={"free_cash":100000,"positions":[{"code":"000001","name":"示例股","cost":10.0,"buy_dt":"20260101","shares":1000,"strategy":"示例策略"}]}
# total_equity 可选；不填时会按 free_cash + 持仓最新市值自动推导
# 读取顺序：Supabase USER_LIVE 优先；若 USER_LIVE 不可用，回退到 MY_PORTFOLIO_STATE
MY_PORTFOLIO_STATE=
TG_BOT_TOKEN=
TG_CHAT_ID=

# Step3 RAG 防雷（可选，未配置 key 自动跳过）
RAG_VETO_ENABLED=1
TAVILY_API_KEY=
RAG_NEGATIVE_KEYWORDS=
RAG_TIMEOUT=12
RAG_MAX_WORKERS=6
RAG_NEWS_LOOKBACK_DAYS=7
RAG_MAX_RESULTS=5

# Step4 OMS 风控增强
STEP4_ATR_PERIOD=14
STEP4_ATR_MULTIPLIER=2.0

# Funnel 并发模式与性能参数
FUNNEL_EXECUTOR_MODE=thread
FUNNEL_MAX_WORKERS=8

# 定时任务交易日口径（默认20点后才取T+0）
MARKET_CLOSE_HOUR=15
MARKET_DATA_READY_HOUR=20

# 交易日历与截面日期严格对齐
ALLOW_APPROX_TRADE_CALENDAR=0
ENFORCE_TARGET_TRADE_DATE=1

# 日线延迟时的实时快照补偿（默认开启）
SPOT_SNAPSHOT_TTL_SECONDS=20
FUNNEL_ENABLE_SPOT_PATCH=1
FUNNEL_SPOT_PATCH_RETRIES=2
FUNNEL_SPOT_PATCH_SLEEP=0.2
STEP3_ENABLE_SPOT_PATCH=1
STEP3_SPOT_PATCH_RETRIES=2
STEP3_SPOT_PATCH_SLEEP=0.2
STEP4_ENABLE_SPOT_PATCH=1
STEP4_SPOT_PATCH_RETRIES=2
STEP4_SPOT_PATCH_SLEEP=0.3

# Step4 开盘跳空保护（动态滑点 = max(静态滑点, ATR*系数)）
STEP4_ATR_SLIPPAGE_FACTOR=0.25
